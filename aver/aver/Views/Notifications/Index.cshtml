@model aver.Models.NotificationViewModel

@{
    ViewBag.Title = "Index";
}

<h2>Notifications</h2>
<div class="container">
    <div class="col-md-6">
        <ul>
            @foreach (var item in Model.Items)
            {
              
                <li style="display:block;">
                    <div>
                        <h4>@item.Title</h4>
                        <ul>
                            <li>@item.Quantity</li>
                            <li>@item.Strength</li>
                            <li>@item.Use</li>
                            <li> @item.Date</li>
                            <li>@item.Instructions</li>
                        </ul>
                        <p> </p>
                        <p> </p>
                        <p> s</p>

                        <p></p>
                        <p></p>
                    </div>

                </li>
            }
        </ul>
    </div>
</div>


@model aver.Models.NotificationViewModel

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<div class="container">
    <div class="col-md-6">
        <ul>
            @foreach (var item in Model.Items)
            {
                <li style="display:block;">
                    <div>
                        <h4>@item.Title</h4>

                        <p>@item.Desctiption</p>
                        <p> @item.Date</p>
                    </div>

                </li>
            }
        </ul>
    </div>
</div>

